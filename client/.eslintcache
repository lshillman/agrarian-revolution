        <button className=\"delete-veggie-btn\" id={req._id} onClick={(e) => displayResponses(e)}>\n                                                <Link to={`/requests/${req._id}`}>\n                                                    Respond\n                                                </Link>\n                                            </button>\n                                        </>\n                                    ))}\n                                </div>\n                            </div>\n                        }\n                        return <></>\n                    })}\n                </div>)\n            }\n        </main>\n    )\n}\n// populate page with incoming requests (requests in my veggies)","/Users/laura/Downloads/agrarian-revolution/client/src/components/VeggiePopup.js",[],"/Users/laura/Downloads/agrarian-revolution/client/src/components/VeggiesList.js",[],"/Users/laura/Downloads/agrarian-revolution/client/src/utils/queries.js",[],"/Users/laura/Downloads/agrarian-revolution/client/src/utils/icons.js",[],"/Users/laura/Downloads/agrarian-revolution/client/src/utils/auth.js",[],"/Users/laura/Downloads/agrarian-revolution/client/src/utils/mutations.js",[],"/Users/laura/Downloads/agrarian-revolution/client/src/components/VeggieRequests.js",[],"/Users/laura/Downloads/agrarian-revolution/client/src/components/VeggieResponses.js",[],{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":6,"column":8,"nodeType":"97","messageId":"98","endLine":6,"endColumn":20},{"ruleId":"95","severity":1,"message":"99","line":4,"column":8,"nodeType":"97","messageId":"98","endLine":4,"endColumn":23},{"ruleId":"95","severity":1,"message":"100","line":18,"column":12,"nodeType":"97","messageId":"98","endLine":18,"endColumn":25},{"ruleId":"95","severity":1,"message":"101","line":20,"column":12,"nodeType":"97","messageId":"98","endLine":20,"endColumn":17},"no-native-reassign",["102"],"no-negat